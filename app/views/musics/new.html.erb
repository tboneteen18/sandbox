<h1>Add a new song</h1>
<%= form_for(@music) do |f|%>
    <p>Title: <%= f.text_field(:title) %></p>
    <p>Artist: <%= f.text_field(:artist)%></p>
    <p>Album: <%= f.text_field(:album)%></p>
    <p>Released At: <%= f.datetime_select(:released_at, start_year: 1980, end_year:Time.now.year)%></p>
    <p><%= f.submit('Save the song')%></p>
<% end %>
